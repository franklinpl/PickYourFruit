</h1> : ''}\n            <form onSubmit={(e) => submitEmail(e)}>\n                <label htmlFor='name'>Name</label>\n                <input type='text' onChange={(e) => setName(e.target.value)} value={name} name='name'/>\n\n                <label htmlFor='email'>Email</label>\n                <input type='email' onChange = {(e) => setEmail(e.target.value)}  name='email' value={email}/>\n\n                <label htmlFor='message'>Message</label>\n                <textarea rows='5' cols='48' onChange = {(e) => setMessage(e.target.value)} name='message' value={message}/>\n\n                <button>Submit</button>\n            </form>\n        </div>\n    )\n}\n\nexport default Contact","import './NavigationMenu.css'\nimport {Link} from 'react-router-dom'\n\nfunction NavigationMenu({show, changeShow}){\n    return(\n        <div>\n            {show ? <div className='navigation-menu'>\n                <Link to='/products' onClick={() => changeShow()}>\n                    <h2>Products</h2>\n                </Link>\n\n                <Link to='/contact' onClick={() => changeShow()}>\n                    <h2>Contact us</h2>\n                </Link>\n           \n                <Link to='/cart' onClick={() => changeShow()}>\n                    <h2>Cart</h2>\n                </Link>\n        </div>   : ''}\n        </div>  \n    )\n}\n\nexport default NavigationMenu","import './HomeScreen.css'\nimport {Link} from 'react-router-dom'\n\nfunction HomeScreen(){\n    return(\n        <div className='homescreen'>\n            <h2>\n                We deliver Scottish fairtrade fruits to you,  \n                <span> Everyday</span>\n            </h2>\n            <Link to='/products'>\n                <button className='homescreen-button'>Check our products</button>\n            </Link>\n           \n        </div>\n    )\n}\n\nexport default HomeScreen","export const ADD_TO_CART = 'ADD_TO_CART'\nexport const REMOVE_FROM_CART = 'REMOVE_FROM_CART'\nexport const ADJUST_QTY = 'AD